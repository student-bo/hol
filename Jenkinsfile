pipeline{
    agent any
    stages {
        stage('Hello'){
            steps{
                echo "Hello World"
                sleep 5
            }
        }
        stage('Build'){
            steps{
                echo "build step"
                sleep 5
            }
        }
        stage('Deploy'){
            steps{
                echo "deploy step"
                sleep 5
            }
        }
        stage('Test'){
            steps{
                echo "test step"
                sleep 5
            }
        }
    }
}
